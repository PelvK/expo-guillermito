{
  "cli": {
    "version": ">= 10.2.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "scheme": "EFIGuillermitoDev"
      },
      "env": {
        "ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "scheme": "EFIGuillermitoPrev"
      },
      "env": {
        "ENV": "preview"
      }
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "simulator": false,
        "scheme": "EFIGuillermito"
      },
      "env": {
        "ENV": "production"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "efi.humboldt@gmail.com",
        "ascAppId": "6748403998" 
      }
    }
  }
}
