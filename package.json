{
  "name": "expo-guillermito",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "dev": "expo start",
    "android-build-development": "eas build --profile development --platform android --local --output ./builds/android-dev.apk",
    "android-build-preview": "eas build --profile preview --platform android --local --output ./builds/android-preview.apk",
    "android-update-development": "eas build --profile development --platform android --local --output ./builds/android-dev.apk",
    "update:dev": "ENV=development eas update --branch dev",
    "update:preview": "ENV=preview eas update --branch preview",
    "update:prod": "ENV=production eas update --branch main",
    "ios-build-development": "eas build --profile development --platform ios --local --output ./builds/ios-dev.app",
    "ios-build-preview": "eas build --profile preview --platform ios --local --output ./builds/ios-preview.app"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
